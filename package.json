{
    "name": "edgaze-prod",
    "version": "1.0.0",
    "description": "Create, sell, and distribute AI products. A curated marketplace for prompts and workflows. [Proprietary]",
    "private": true,
    "type": "module",
    "author": "Edgaze",
    "license": "UNLICENSED",
    "keywords": [
        "ai",
        "prompts",
        "workflows",
        "marketplace",
        "nextjs",
        "typescript",
        "supabase"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint . || true",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "clean": "rm -rf .next node_modules",
        "reinstall": "npm run clean && npm install",
        "db:types": "npx supabase gen types typescript --project-id $NEXT_PUBLIC_SUPABASE_PROJECT_ID > src/types/supabase.ts",
        "db:push": "npx supabase db push",
        "db:reset": "npx supabase db reset"
    },
    "dependencies": {
        "@supabase/ssr": "^0.8.0",
        "@supabase/supabase-js": "^2.84.0",
        "@vercel/analytics": "^1.6.1",
        "clsx": "^2.1.1",
        "framer-motion": "^12.23.24",
        "lucide-react": "^0.452.0",
        "mixpanel-browser": "^2.73.0",
        "next": "^16.0.1",
        "next-auth": "^4.24.13",
        "qrcode": "^1.5.4",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-resizable-panels": "^3.0.6",
        "reactflow": "^11.11.4"
    },
    "devDependencies": {
        "@types/node": "^20.14.12",
        "@types/qrcode": "^1.5.6",
        "@types/react": "^19.0.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^10.0.0",
        "eslint-config-next": "^16.1.1",
        "postcss": "^8.4.45",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.6.2"
    },
    "engines": {
        "node": ">=20"
    }
}
